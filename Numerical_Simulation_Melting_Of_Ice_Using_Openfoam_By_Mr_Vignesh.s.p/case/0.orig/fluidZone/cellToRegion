FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    arch        "LSB;label=32;scalar=64";
    location    "0/fluidZone";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];


internalField   uniform 0;

boundaryField
{
    fluidFrontAndBack
    {
        type            zeroGradient;
    }
    fluidZone_to_solidZone
    {
        type            calculated;
        value           uniform 0;
    }
}


